    "lint:fix": "eslint . --ext .ts --fix"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@hazmi35/eslint-config/typescript"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.x"
  }
}
