{
  "name": "ytapi",
  "version": "0.0.0",
  "description": "Simple YouTube API wrapper writen in TypeScript",
  "keywords": [
    "nodejs",
    "javascript",
    "youtube",
    "google",
    "typescript",
    "youtube-api",
    "youtubeapi",
    "youtubeapiv3",
    "ytapi"
  ],
  "homepage": "https://github.com/Hazmi35/node-ytapi#readme",
  "bugs": {
    "url": "https://github.com/Hazmi35/node-ytapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hazmi35/node-ytapi.git"
  },
  "license": "MIT",
  "author": "Hazmi35",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn run lint && yarn run compile",
    "compile": "tsc --build tsconfig.json",
